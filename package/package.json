{
  "name": "react-machine-package",
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./types": {
      "import": "./dist/types.js",
      "types": "./dist/types.d.ts"
    },
    "./core-components": {
      "import": "./dist/core-components.js",
      "types": "./dist/core-components.d.ts"
    },
    "./core-api": {
      "import": "./dist/core-api.js",
      "types": "./dist/core-api.d.ts"
    },
    "./devices-components": {
      "import": "./dist/devices-components.js",
      "types": "./dist/devices-components.d.ts"
    },
    "./devices-api": {
      "import": "./dist/devices-api.js",
      "types": "./dist/devices-api.d.ts"
    },
    "./resources/*": "./dist/resources/*",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "vite build",
    "build:vite": "vite build",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir dist"
  },
  "devDependencies": {
    "@types/node": "^25.0.10",
    "@types/react": "^19.2.9",
    "@vitejs/plugin-react": "^4.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-static-copy": "^3.1.5"
  },
  "peerDependencies": {
    "react": ">=18 <20",
    "react-dom": ">=18 <20"
  },
  "dependencies": {
    "eventemitter3": "^5.0.4",
    "prism-react-editor": "^3.0.1",
    "wouter": "^3.9.0"
  },
  "files": [
    "dist",
    "package.json"
  ]
}
